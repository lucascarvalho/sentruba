{
  "name": "sentruba",
  "version": "0.0.1",
  "description": "Desktop notifications for Sentry",
  "main": "main.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint -c .eslintrc .",
    "start:electron": "NODE_ENV=development electron -r @babel/register .",
    "start:webpack": "NODE_ENV=development webpack-dev-server",
    "test": "jest --verbose"
  },
  "lint-staged": {
    "*.js": [
      "eslint -c .eslintrc --fix"
    ]
  },
  "repository": "https://github.com/lucascarvalho/sentruba",
  "keywords": [
    "Sentruba",
    "Sentry",
    "notifications",
    "desktop"
  ],
  "author": "Lucas Carvalho Teixeira",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.50",
    "@babel/register": "7.0.0-beta.46",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.0",
    "css-loader": "0.28.11",
    "electron": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-reload": "1.2.2",
    "eslint": "4.19.1",
    "eslint-plugin-jest": "21.15.1",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "jest": "22.4.3",
    "lint-staged": "7.1.0",
    "node-sass": "4.9.0",
    "prop-types": "15.6.1",
    "react-hot-loader": "4.1.3",
    "regenerator-runtime": "0.11.1",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "webpack": "4.8.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "history": "4.7.2",
    "humps": "2.0.1",
    "isomorphic-fetch": "2.2.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-form": "7.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "timers": "fake",
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "verbose": true
  }
}
