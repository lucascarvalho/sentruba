{
  "name": "sentruba",
  "version": "0.0.1",
  "description": "Desktop notifications for Sentry",
  "main": "main.js",
  "scripts": {
    "precommit": "lint-staged",
    "lint": "eslint -c .eslintrc .",
    "start": "NODE_ENV=development electron -r babel-register .",
    "watch": "NODE_ENV=development node server.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint -c .eslintrc --fix"
    ]
  },
  "repository": "https://github.com/lucascarvalho/sentruba",
  "keywords": [
    "Sentruba",
    "Sentry",
    "notifications",
    "desktop"
  ],
  "author": "Lucas Carvalho Teixeira",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "css-loader": "0.28.7",
    "electron": "1.7.9",
    "electron-reload": "1.2.2",
    "eslint": "4.9.0",
    "eslint-plugin-jest": "21.2.0",
    "eslint-plugin-react": "7.4.0",
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "node-sass": "4.5.3",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-hot-loader": "3.1.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.3"
  }
}
